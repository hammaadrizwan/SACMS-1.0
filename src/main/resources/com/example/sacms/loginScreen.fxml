    <ImageView blendMode="MULTIPLY" fitHeight="58.0" fitWidth="200.0" layoutX="27.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@../images/Logo.png" />
        </image>
    </ImageView>
      <AnchorPane layoutX="331.0" layoutY="77.0" prefHeight="534.0" prefWidth="413.0" style="-fx-background-color: white; -fx-background-radius: 25;">
