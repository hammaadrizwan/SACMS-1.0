<AnchorPane prefHeight="680.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sacms.Controller">
   <children>
      <ImageView fitHeight="680.0" fitWidth="1050.0" pickOnBounds="true">
         <image>
            <Image url="@../images/SignInScreen0BG.png" />
