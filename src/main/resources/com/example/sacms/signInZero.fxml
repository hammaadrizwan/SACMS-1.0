         <image>
            <Image url="@../images/Logo.png" />
         </image>
      </ImageView>

      <AnchorPane layoutX="298.0" layoutY="55.0" prefHeight="573.0" prefWidth="477.0" style="-fx-background-color: #ffffff; -fx-background-radius: 20;">
         <children>
            <Text layoutX="134.0" layoutY="94.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register as..">
               <font>
                  <Font name="System Bold" size="42.0" />
               </font>
            </Text>
            <Button layoutX="60.0" layoutY="287.0" mnemonicParsing="false" onAction="#onStudentSignInOneButtonClicked" prefHeight="67.0" prefWidth="171.0" style="-fx-background-color: #F2DEB8; -fx-background-radius: 10;" text="STUDENT">
               <font>
                  <Font name="Noto Sans Bold" size="20.0" />
